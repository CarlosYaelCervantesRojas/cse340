<header id="top-header">
    <span class="siteName">
        <a href="/" title="Return to home page">CSE Motors</a>
    </span>
    <div id="tools">
        <% if (locals.loggedin) { %>
            <a href="/account/">Welcome <%= locals.accountData.account_firstname %></a> | <a id="logout-link" href="#">Logout</a>
            <form action="/account/logout" method="post" id="logout-form"></form>
        <% } else { %>
            <a title="Click to log in" href="/account/login">My Account</a> 
        <% } %>
    </div>
</header>

<script src="/js/logout.js"></script>